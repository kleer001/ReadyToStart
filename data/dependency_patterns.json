  },
  "density_modifiers": {
    "low": 0.5,
    "medium": 1.0,
    "high": 1.5,
    "very_high": 2.0,
    "chaotic": 2.5,
    "paradoxical": 3.0,
    "recursive": 3.5
  },
  "special_category_rules": {
    "meta_settings": {
      "always_include": ["circular_hint", "chain_dependency"],
      "modifier": 1.5
    },
    "quantum_settings": {
      "always_include": ["circular_hint"],
      "modifier": 1.3
    },
    "experimental": {
      "always_include": ["chaotic_dependencies"],
      "modifier": 1.8
    },
    "developer": {
      "always_include": ["combo_requirement", "group_requirement"],
      "modifier": 1.4
    }
  }
}
