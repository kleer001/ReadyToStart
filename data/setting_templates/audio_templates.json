{
  "category": "audio",
  "name": "Audio Settings",
  "templates": [
    {
      "id_pattern": "audio_enable",
      "type": "boolean",
      "label_template": "Enable {category_name}",
      "default_value": false,
      "importance": "critical",
      "dependency_likelihood": 0.95
    },
    {
      "id_pattern": "audio_advanced_mode",
      "type": "boolean",
      "label_template": "Show Advanced Options",
      "default_value": false,
      "importance": "medium",
      "dependency_likelihood": 0.8
    },
    {
      "id_pattern": "audio_volume",
      "type": "integer",
      "label_template": "{adjective} Volume",
      "min_value": 0,
      "max_value": 100,
      "default_value": 50,
      "importance": "high"
    },
    {
      "id_pattern": "audio_device",
      "type": "string",
      "label_template": "{device} Selection",
      "options": [
        "default",
        "speakers",
        "headphones",
        "hdmi",
        "optical"
      ],
      "default_value": "default",
      "importance": "high"
    },
    {
      "id_pattern": "audio_sample_rate",
      "type": "integer",
      "label_template": "Sample Rate",
      "min_value": 22050,
      "max_value": 192000,
      "default_value": 44100,
      "importance": "medium"
    },
    {
      "id_pattern": "audio_channels",
      "type": "string",
      "label_template": "Channel Configuration",
      "options": [
        "mono",
        "stereo",
        "surround_5_1",
        "surround_7_1",
        "atmos"
      ],
      "default_value": "stereo",
      "importance": "medium"
    },
    {
      "id_pattern": "audio_setting_100",
      "type": "integer",
      "label_template": "{noun} allocation limit",
      "importance": "high",
      "min_value": 0,
      "max_value": 300,
      "default_value": 150
    },
    {
      "id_pattern": "audio_setting_101",
      "type": "boolean",
      "label_template": "Enable {feature} support",
      "importance": "low",
      "default_value": false
    },
    {
      "id_pattern": "audio_setting_102",
      "type": "boolean",
      "label_template": "Enable {feature} support",
      "importance": "low",
      "default_value": false
    },
    {
      "id_pattern": "audio_setting_103",
      "type": "integer",
      "label_template": "Maximum {noun} count",
      "importance": "medium",
      "min_value": 0,
      "max_value": 300,
      "default_value": 150
    },
    {
      "id_pattern": "audio_setting_104",
      "type": "integer",
      "label_template": "{adjective} {noun} level",
      "importance": "high",
      "min_value": 0,
      "max_value": 300,
      "default_value": 150
