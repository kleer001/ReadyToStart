    {
      "id_pattern": "audio_setting_106",
      "type": "integer",
      "label_template": "Maximum {noun} count",
      "importance": "low",
      "min_value": 0,
      "max_value": 300,
      "default_value": 150
    },
    {
      "id_pattern": "audio_setting_107",
      "type": "boolean",
      "label_template": "Enable {adjective} {noun}",
      "importance": "medium",
      "default_value": false
